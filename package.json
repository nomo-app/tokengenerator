{
  "name": "tokengenerator",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "build_tar": "rm -f $npm_package_name.tar.gz && COPYFILE_DISABLE=1 tar -cv --gzip --file=$npm_package_name.tar.gz out",
    "start": "npx serve@latest out",
    "lint": "next lint",
    "generate_docs": "typedoc --plugin typedoc-plugin-markdown --out ../api-docs/ src/app/nomo-plugin-kit/nomo_api.ts && rm ../api-docs/README.md",
    "deploy": "./scripts/deploy.sh",
    "deploy_tar": "./scripts/deploy_tar.sh",
    "clear:cache": "./scripts/clear_cloudflare_cache.sh"
  },
  "dependencies": {
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "ethers": "5.5.4",
    "ethersjs-nomo-plugins": "^0.1.1",
    "next": "13.4.3",
    "nomo-plugin-kit": "^0.1.3",
    "react": "18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-qr-code": "^2.0.12",
    "sass": "^1.66.1",
    "string-similarity": "^4.0.4",
    "string-similarity-algorithm": "^1.1.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/string-similarity": "^4.0.0",
    "typedoc": "0.24.7",
    "typedoc-plugin-markdown": "3.15.3"
  }
}